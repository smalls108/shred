<div class = "container py-5">
    <% @instructors.each do |instructor| %>
    <div class="col-md-8">
        <div class="card-body">
        <h5 class="card-title"><%= link_to instructor.user.first_name, instructor_path(instructor)  %></h5>
        <p class="card-text"><%= instructor.description %></p>     
    </div>
    <% end %>
</div>
